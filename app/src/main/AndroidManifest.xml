<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.unitconversion">
<uses-permission android:name="android.permission.INTERNET"/>
    <application

        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UnitConversion"
        tools:targetApi="31">
        <activity
            android:name=".setting"
            android:exported="false" />
        <activity
            android:name=".AboutMe"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".inchesToMm"
            android:exported="true"> </activity>
        <activity
            android:name=".mmToInches"
            android:exported="true"> </activity>
        <activity
            android:name=".feetToMeters"
            android:exported="true"> </activity>
        <activity
            android:name=".meterToFeets"
            android:exported="true"> </activity>
        <activity
            android:name=".yardsToMeter"
            android:exported="true"> </activity>
        <activity
            android:name=".metersToYards"
            android:exported="true"> </activity>
        <activity
            android:name=".milesToKm"
            android:exported="true"> </activity>
        <activity
            android:name=".kmToMiles"
            android:exported="true"> </activity>
        <activity
            android:name=".moreConversionTools"
            android:exported="true"> </activity>
        <activity
            android:name=".temperaturePanel"
            android:exported="true"> </activity>
        <activity
            android:name=".massConvertor"
            android:exported="true"> </activity>
        <activity
            android:name=".computerConversion"
            android:exported="true"> </activity>
        <activity
            android:name="com.example.unitconversion.speedConvertor"
            android:exported="true"> </activity>
    </application>

</manifest>